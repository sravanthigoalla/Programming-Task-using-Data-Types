    "1.print the list in lexicographical increasing order\n",
    "\n",
    "x = int(input())\n",
    "y = int(input())\n",
    "z = int(input())\n",
    "n = int(input())\n",
    "l = list()\n",
    "for i in range(x+1):\n",
    "    for j in range(y+1):\n",
    "        for k in range (z+1):\n",
    "            if(i+j+k!=n):\n",
    "                l.append([i,j,k])\n",
    "print (l)   "
